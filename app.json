{
  "expo": {
    "name": "BetterMind",
    "slug": "better-mind",
    "version": "1.0.0",
    "privacy": "public",
    "description": "For testing purpose ",
    "orientation": "portrait",
    "icon": "./assets/images/transparent.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.developertyzend.bettermind",
      "userInterfaceStyle": "light",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appIcon.png",
        "backgroundColor": "#ffffff",
        "userInterfaceStyle": "light"
      },
      "package": "com.developertyzend.bettermind",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-av"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "116b854c-f192-4c1f-9713-34e7b598db97"
      }
    },
    "owner": "hamza-4095"
  }
}
